{
  "name": "HLTV Google Calendar",
  "description": "A Chrome extension to add CSGO matches listed on HLTV.org to your Google Calendar",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "./popup/popup.html"
  },
  "icons": {
    "16": "icons/favicon-16x16.png",
    "48": "icons/favicon-48x48.png",
    "144": "icons/favicon-144x144.png"
  },
  "permissions": ["https://www.hltv.org/matches/*"]
}
